/* First entry defines the entire thing parsed */
expression: add;
add: multiply;
add: multiply "+" add {binop};
add: multiply "-" add {binop};
multiply: number;
multiply: number "*" multiply {binop};
multiply: number "/" multiply {binop};
digit: "0";
digit: "1";
digit: "2";
digit: "3";
digit: "4";
digit: "5";
digit: "6";
digit: "7";
digit: "8";
digit: "9";
integer: digit;
integer: digit integer {stitch};
number: integer {makeint};
/* number: float {makefloat}; */
